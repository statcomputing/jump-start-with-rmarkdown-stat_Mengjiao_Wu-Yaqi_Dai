Bias  <- matrix(data = NA, nrow = 3, ncol = 100 )
true_value <- pnorm(0)
i=1
for(N in N_set){
  for(j in 1:100){
    x <- rnorm(N, mean=0, sd=1)
    Indicator_variable <- as.numeric(x <= 0)
    Fi_t = mean(Indicator_variable)
    Bias[i,j] <- Fi_t-true_value
  }
 i=i+1
  
}
c1 <- Bias[1,]
